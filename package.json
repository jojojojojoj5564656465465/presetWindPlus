{
  "name": "preset-wind-plus",
  "type": "module",
  "version": "2.1.5",
  "packageManager": "pnpm@8.7.0",
  "description": "<description>",
  "author": "unpreset <https://github.com/unpreset>",
  "license": "MIT",
  "homepage": "https://github.com/unpreset/unocss-preset-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unpreset/unocss-preset-starter.git"
  },
  "bugs": {
    "url": "https://github.com/unpreset/unocss-preset-starter/issues"
  },
  "keywords": [
    "unocss",
    "unpreset",
    "unocss-preset",
    "unocss-preset-starter"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "lint": "bunx biome lint --write --unsafe ./src ; bunx biome format --write ./src",
    "test": "vitest",
    "test:update": "vitest --update",
    "play": "vite playground",
    "deploy": "vite build playground",
    "testcode": "bun --watch run src/testcode.ts"
  },
  "peerDependencies": {
    "@unocss/core": "^0.58.9"
  },
  "dependencies": {
    "string-ts": "^2.2.0",
    "valibot": "^0.42.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@babel/types": "^7.26.0",
    "@types/node": "^22.10.1",
    "@unocss/eslint-plugin": "^0.65.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "bumpp": "^9.8.1",
    "eslint": "^9.16.0",
    "typescript": "5.3.3",
    "unbuild": "^2.0.0",
    "unocss": "^0.58.9",
    "vite": "^6.0.2",
    "vitest": "^1.6.0"
  },
  "wireit": {
    "build": {
      "command": "unbuild",
      "output": [
        "dist"
      ]
    },
    "release": {
      "command": "npm publish --no-git-checks",
      "clean": false,
      "dependencies": [
        "format"
      ],
      "output": [
        "dist"
      ]
    },
    "format": {
      "command": "bun x @biomejs/biome format --write ."
    },
    "dev": {
      "command": "bun --watch run test.ts",
      "clean": true
    }
  },
  "directories": {
    "doc": "doc",
    "test": "test"
  }
}
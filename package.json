{
  "name": "preset-wind-plus",
  "type": "module",
  "version": "2.0.20",
  "packageManager": "pnpm@8.7.0",
  "description": "<description>",
  "author": "unpreset <https://github.com/unpreset>",
  "license": "MIT",
  "homepage": "https://github.com/unpreset/unocss-preset-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unpreset/unocss-preset-starter.git"
  },
  "bugs": {
    "url": "https://github.com/unpreset/unocss-preset-starter/issues"
  },
  "keywords": [
    "unocss",
    "unpreset",
    "unocss-preset",
    "unocss-preset-starter"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "lint": "bunx @biomejs/biome check --apply ./src",
    "test": "vitest",
    "test:update": "vitest --update",
    "play": "vite playground",
    "deploy": "vite build playground",
    "testcode": "bun --watch run src/testcode.ts"
  },
  "peerDependencies": {
    "@unocss/core": "< 1"
  },
  "dependencies": {
    "string-ts": "^2.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.6",
    "@babel/types": "^7.23.6",
    "@types/node": "^20.10.4",
    "@unocss/eslint-plugin": "^0.58.0",
    "bumpp": "^9.2.1",
    "eslint": "^8.56.0",
    "typescript": "5.3.3",
    "unbuild": "^2.0.0",
    "unocss": "^0.58.0",
    "vite": "^5.0.10",
    "vitest": "^1.0.4"
  },
  "wireit": {
    "build": {
      "command": "unbuild",
      "output": [
        "dist"
      ]
    },
    "release": {
      "command": "npm publish --no-git-checks",
      "clean": false,
      "dependencies": [
        "format"
      ],
      "output": [
        "dist"
      ]
    },
    "format": {
      "command": "bun x @biomejs/biome format --write ."
    },
    "dev": {
      "command": "bun --watch run test.ts",
      "clean": true
    }
  },
  "directories": {
    "doc": "doc",
    "test": "test"
  }
}

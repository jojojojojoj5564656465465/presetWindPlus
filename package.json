{
  "name": "preset-wind-plus",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@8.7.0",
  "description": "<description>",
  "author": "unpreset <https://github.com/unpreset>",
  "license": "MIT",
  "homepage": "https://github.com/unpreset/unocss-preset-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unpreset/unocss-preset-starter.git"
  },
  "bugs": {
    "url": "https://github.com/unpreset/unocss-preset-starter/issues"
  },
  "keywords": [
    "unocss",
    "unpreset",
    "unocss-preset",
    "unocss-preset-starter"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:update": "vitest --update",
    "play": "vite playground",
    "deploy": "vite build playground"
  },
  "peerDependencies": {
    "@unocss/core": "< 1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.6",
    "@babel/types": "^7.23.6",
    "@types/node": "^20.10.4",
    "@unocss/eslint-plugin": "^0.58.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "bumpp": "^9.2.1",
    "eslint": "^8.56.0",
    "typescript": "5.3.3",
    "unbuild": "^2.0.0",
    "unocss": "^0.58.0",
    "vite": "^5.0.10",
    "vitest": "^1.0.4"
  }
}
